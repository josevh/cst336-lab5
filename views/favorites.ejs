<%- include('partials/header.ejs') %>
<body>
<style>
    body {
        background-repeat: repeat;
        background-size: auto;
    }
</style>
<div id="favContainer">
    <a class="link" href="/">Back to Search</a>
    
    <h1>Favorite Pics</h1>
    
    <%
        // displaying five keywords per line
        rows.forEach(function(row, i) { %>
            <a href="#" class="keywordLink"><%= row["keyword"] %></a>
    
            <%- ((i+1) % 6 === 0) ? '<br><br>' : '' %>
    <% }) %>
    <hr>
    <div id="favorites"></div>
    
    <input type="hidden" id="keywordSelected">

<%- include('partials/footer.ejs') %>

</body>
</html>
